/********************************************
 *   Check if a tree is BST
 ********************************************/

#include<stdio.h>
#include<stdlib.h>

struct Node {
    int data;
    struct Node *left, *right;
};

bool isBST (struct Node * treeRoot) {

    if (treeRoot == NULL) {
        return true;
    }

    if( treeRoot->data > treeRoot->left->data &&
        treeRoot->data <= treeRoot->right->data ) {
        return true;
    }

    return isBST(treeRoot->left) && isBST(treeRoot->right);
}

int main () {

    return 0;
}
